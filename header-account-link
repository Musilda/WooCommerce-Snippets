

if ( is_user_logged_in() ) { 
    echo '<a href="'.get_permalink( get_option('woocommerce_myaccount_page_id') ).'" title="'.__('My Account','woothemes').'">'.__('My Account','woothemes').'</a>';
} else {
    echo '<a href="'.get_permalink( get_option('woocommerce_myaccount_page_id') ).'" title="'.__('Login / Register','woothemes').'">'.__('Login / Register','woothemes').'</a>';
} 
